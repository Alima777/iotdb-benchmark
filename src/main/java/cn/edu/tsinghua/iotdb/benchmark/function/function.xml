	<function id="monok1" function-type="double_mono_k" max="1000000000.5" min="17366.5" cycle="350000"></function>
	<!-- 傅里叶函数 1个方法  都可以转换为正弦函数-->
	<function id="sin1" function-type="double_sin" max="100000000.59887" min="3.54113" cycle="1754"></function>
	
	<!-- 分段常数函数 1个 -->
	<function id="square1" function-type="double_square" max="5" min="2" cycle="182000"></function>
	<function id="square4" function-type="double_square" max="2147483620.42" min="1473.54" cycle="4200000"></function>
	
	<!-- 随机数函数 3 个-->
	<function id="random1" function-type="int_random" max="123456789" min="-55" cycle="18200"></function>
	<function id="random2" function-type="double_random" max="1234567890.89" min="0.0" cycle="18200"></function>

	<!-- 常数函数 3 个-->
	<function id="constant2" function-type="int_mono" max="112151208" min="112151208" cycle="3600"></function>
